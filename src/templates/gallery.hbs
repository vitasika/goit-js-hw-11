{{#each this}}
  <div class='photo-card'>
    <a href='{{largeImageURL}}' class='photo-ref'>
      <img src='{{webformatURL}}' alt='' loading='lazy' class='img-gallery' />
    </a>
    <div class='info'>
      <p class='info-item'>
        <b>Likes</b>
        {{likes}}
      </p>
      <p class='info-item'>
        <b>Views</b>
        {{views}}
      </p>
      <p class='info-item'>
        <b>Comments</b>
        {{comments}}
      </p>
      <p class='info-item shrink'>
        <b>Downloads</b>
        {{downloads}}
      </p>
    </div>
  </div>
{{/each}}